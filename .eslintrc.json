{
  "extends": [
    "oclif",
    "oclif-typescript",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "rules": {
    "space-before-function-paren": "off",
    "indent": "off",
    "operator-linebreak": "off",
    "node/no-missing-require": [
      "error",
      {
        "tryExtensions": [".ts", ".js"]
      }
    ],
    "node/no-missing-import": [
      "error",
      {
        "tryExtensions": [".ts", ".js"]
      }
    ]
  }
}
