{
  "name": "sfdx-md-merge-driver",
  "version": "0.1.6",
  "description": "git merge driver specific for Salesforce.com Metadata",
  "author": "jayree @jayree",
  "bin": {
    "sfdx-md-merge-driver": "./bin/run"
  },
  "bugs": "https://github.com/jayree/sfdx-md-merge-driver/issues",
  "man": "./sfdx-md-merge-driver.1",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3.2.1",
    "mkdirp": "1.0.4",
    "pkg-dir": "5.0.0",
    "shelljs": "0.8.4",
    "tslib": "2.0.3",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/mkdirp": "1.0.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@types/shelljs": "0.8.8",
    "@types/xml2js": "0.4.7",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "chai": "^4.2.0",
    "eslint": "7.17.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "globby": "^11.0.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "marked": "1.2.7",
    "marked-man": "^0.7.0",
    "mocha": "^8.2.1",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "prettier-standard": "^16.4.1",
    "pretty-quick": "3.1.0",
    "source-map-support": "0.5.19",
    "standard": "^16.0.3",
    "standard-version": "^9.1.0",
    "tap": "^14.11.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/conf",
    "sfdx-md-merge-driver.1",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jayree/sfdx-md-merge-driver",
  "keywords": [
    "sfdx",
    "salesforce",
    "metadata",
    "git",
    "merge",
    "driver",
    "conflict",
    "merge conflict",
    "resolve conflict",
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx-md-merge-driver",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-md-merge-driver",
  "scripts": {
    "docs": "tail -n +2 README.md | marked-man --manual 'User Commands' --version \"$npm_package_name@$npm_package_version\" > sfdx-md-merge-driver.1",
    "prerelease": "yarn test",
    "postrelease": "npm publish && git push --follow-tags",
    "release": "standard-version",
    "uninstall": "node ./bin/run uninstall",
    "install": "node ./bin/run install",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "yarn prepack && nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
